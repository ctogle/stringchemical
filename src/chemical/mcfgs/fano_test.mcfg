
<end_criteria>
	time limit : 100

<capture_criteria>
	scalar increment : 1.0 : time

<variables>
	r : 10.0
	b : 100.0
	d : 1.0
	g : .01

<functions>
	y_creation_rate : b*d

<reactions>
	nothing r -> 1 x : x formation
	1 x d -> nothing : x annihilation
	1 x y_creation_rate -> 1 y 1 x : x makes y
	1 y g -> nothing : y annihilation

<species>
	y : 0
	x : 0

